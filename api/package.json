{
  "name": "litedkit-api",
  "version": "2.3.0",
  "description": "Koa api",
  "private": false,
  "main": "src/app.js",
  "author": "Paralect",
  "license": "MIT",
  "keywords": [
    "koa",
    "rest api",
    "paralect"
  ],
  "scripts": {
    "copy-assets": "mkdir -p \"dist/src/assets/emails\" && cp -R src/assets/emails/* dist/src/assets/emails/",
    "build": "run-s build:**",
    "build:email": "npm run copy-assets && mjml ./src/assets/emails/*.mjml -o ./src/assets/emails/dist/",
    "build:tcs": "tsc",
    "serve": "node dist/src/app.js",
    "start": "npm run serve",
    "test": "run-s test:**",
    "test:eslint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "test:mocha": "NODE_ENV=production APP_ENV=test ts-mocha --timeout 20000 --require src/test.ts --recursive --exit -c -R spec 'src/**/*.spec.ts'",
    "development": "nodemon --watch src -e ts,tsx --exec ts-node --project tsconfig.json src/app.ts",
    "add-contributor": "all-contributors add",
    "generate-contributors": "all-contributors generate",
    "migrate": "ts-node ./src/migrator.ts",
    "migration-generate": "ts-node ./src/migrations/generator.ts",
    "schedule-dev": "nodemon --watch src -e ts,tsx --exec ts-node --project tsconfig.json ./src/scheduler.ts",
    "schedule": "node dist/src/scheduler.js",
    "prepush": "lint-prepush --allow-empty"
  },
  "lint-prepush": {
    "tasks": {
      "*.js": [
        "eslint --fix"
      ]
    }
  },
  "_moduleAliases": {},
  "dependencies": {
    "@aws-sdk/client-s3": "3.18.0",
    "@aws-sdk/s3-request-presigner": "3.18.0",
    "@aws-sdk/util-create-request": "3.18.0",
    "@aws-sdk/util-format-url": "3.18.0",
    "@koa/cors": "3.1.0",
    "@koa/multer": "3.0.0",
    "@koa/router": "9.4.0",
    "@paperclip/api-health": "0.0.7",
    "@paperclip/node-mongo": "3.0.34",
    "@paperclip/utils": "0.0.8",
    "@paralect/common-logger": "0.1.10",
    "@paralect/node-mongo": "2.0.0-beta.7",
    "@sendgrid/mail": "7.3.0",
    "@socket.io/redis-adapter": "7.1.0",
    "@socket.io/redis-emitter": "4.1.1",
    "app-module-path": "2.2.0",
    "axios": "0.21.2",
    "bluebird": "3.7.2",
    "csv-parse": "4.15.4",
    "google-auth-library": "6.0.6",
    "hot-formula-parser": "4.0.0",
    "joi": "17.4.2",
    "koa": "2.13.0",
    "koa-bodyparser": "4.3.0",
    "koa-helmet": "5.2.0",
    "koa-logger": "3.2.1",
    "koa-mount": "4.0.0",
    "koa-qs": "3.0.0",
    "koa-validate": "1.0.7",
    "lodash": "4.17.21",
    "mjml": "4.7.0",
    "module-alias": "2.2.2",
    "moment": "2.29.0",
    "moment-duration-format": "2.3.2",
    "moment-timezone": "0.5.32",
    "multer": "1.4.2",
    "node-schedule": "1.3.2",
    "nodemon": "2.0.4",
    "npm-run-all": "4.1.5",
    "plaid": "9.1.0",
    "psl": "1.8.0",
    "redis": "4.0.2",
    "shortid": "2.2.15",
    "socket.io": "4.2.0",
    "socket.io-emitter": "3.2.0",
    "socket.io-redis": "6.1.0",
    "string-similarity": "4.0.3",
    "ts-mocha": "9.0.2",
    "winston": "3.3.3",
    "winston-loggly-bulk": "3.1.1"
  },
  "devDependencies": {
    "@faker-js/faker": "6.0.0-alpha.3",
    "@types/bluebird": "3.5.36",
    "@types/chai": "4.3.0",
    "@types/faker": "6.6.9",
    "@types/koa": "2.13.4",
    "@types/koa__cors": "3.1.1",
    "@types/koa__multer": "2.0.4",
    "@types/koa__router": "8.0.11",
    "@types/koa-bodyparser": "4.3.5",
    "@types/koa-helmet": "6.0.4",
    "@types/koa-logger": "3.1.2",
    "@types/koa-mount": "4.0.1",
    "@types/koa-qs": "2.0.0",
    "@types/lodash": "4.14.178",
    "@types/mocha": "9.1.0",
    "@types/module-alias": "2.0.1",
    "@types/moment-timezone": "0.5.30",
    "@types/node-schedule": "1.3.2",
    "@types/psl": "1.1.0",
    "@types/redis": "4.0.11",
    "@types/shortid": "0.0.29",
    "@types/socket.io": "3.0.2",
    "@types/socket.io-redis": "3.0.0",
    "@types/string-similarity": "4.0.0",
    "@types/supertest": "2.0.11",
    "@types/winston-loggly-bulk": "3.0.3",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "all-contributors-cli": "6.17.4",
    "chai": "4.2.0",
    "eslint": "8.3.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-plugin-import": "2.25.3",
    "faker": "6.6.6",
    "lint-prepush": "2.2.0",
    "mocha": "8.1.3",
    "supertest": "4.0.2",
    "ts-migrate": "0.1.27",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  }
}
